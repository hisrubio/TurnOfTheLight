{
  "manifest_version": 2,
  "name": "Mi Extension",
  "version": "1.0",
 
  "description": "A plain text description",
   "background": {
    "scripts": ["background.js"]
    
  },"icons": { 
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png" 
  },

  "browser_action": {
    "default_popup" : "popup.html",
    "default_title" : "mi Extension"
    },
   "permissions": [
      "tabs", "http://*/*", "https://*/*", "alarms"
  ]
}